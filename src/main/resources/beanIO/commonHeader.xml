<?xml version="1.0" encoding="UTF-8"?>
<beanio xmlns="http://www.beanio.org/2012/03" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.beanio.org/2012/03 http://www.beanio.org/2012/03/mapping.xsd">
  <stream name="request" format="fixedlength" strict="true">
    <parser>
      <property name="recordTerminator" value=""/>
    </parser>
    <record name="commonHeader" class="com.lottecard.myd.cmn.CommonHeader" maxOccurs="1">
      	<field name="gramLnth" length="5" padding="0" justify="right" type="int"/>
        <field name="guid" length="31" justify="left" padding=" " type="String"/>
        <field name="gramPrgNo" length="2" padding="0" justify="right" type="int"/>
        <field name="gramNo" length="8" justify="right" padding="0" type="String"/>
        <field name="akRspDc" length="1" justify="right" padding=" " type="String"/>
        <field name="rspBizDc" length="2" justify="left" padding=" " type="String"/>
    </record>
  </stream>

  <stream name="requestData" format="fixedlength" strict="true">
    <parser>
      <property name="recordTerminator" value=""/>
    </parser>
    <record name="commonData" class="com.lottecard.myd.cmn.CommonData" maxOccurs="1">
      <field name="cno" length="22" justify="left" padding=" " type="String"/>
      <field name="rcvDenyChnlRgIzCnt" length="3" padding="0" justify="right" type="int"/>
          <segment name="rcvDenyChnlRgIz" class="com.lottecard.myd.cmn.RcvDenyChnlRgIz" collection="list" minOccurs="1" maxOccurs="unbounded">
                <field name="rcvDenyYn" length="2" justify="left" padding=" " type="String"/>
		        <field name="rgDt" length="16" justify="left" padding=" " type="String"/>
		        <field name="rgrEno" length="14" justify="left" padding=" " type="String"/>
		        <field name="rgrNm" length="40" justify="left" padding=" " type="String"/>
		        <field name="chnlKndc" length="2" justify="left" padding=" " type="String"/>
		        <field name="rcvDenyCnC" length="4" justify="left" padding=" " type="String"/>
	      </segment>
    </record>
  </stream>
</beanio>