        <field name="gramPrgNo" length="2" padding="0" justify="right" type="int"/>
        <field name="gramNo" length="8" justify="right" padding="0" type="String"/>
        <field name="akRspDc" length="1" justify="right" padding=" " type="String"/>
        <field name="rspBizDc" length="2" justify="left" padding=" " type="String"/>
    </record>
  </stream>
</beanio>